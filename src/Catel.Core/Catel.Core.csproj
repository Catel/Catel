    <ProjectGuid>3B47FC74-57AD-4F8B-B9C6-7F31FF39687B</ProjectGuid>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Catel.Fody" Version="4.3.0" PrivateAssets="all" />
